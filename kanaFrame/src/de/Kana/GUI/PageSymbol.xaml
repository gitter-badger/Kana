<Page x:Class="KanaFrame.PageSymbol"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KanaFrame"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="PageSymbol">
    <Grid>
        <StackPanel>
            <Border>
                <StackPanel Orientation="Horizontal" >
                    <Button Content="Back" Height="30" Click="button1_Click"/>
                    <Button Content="Settings" Click="button2_Click"/>
                    <Button Content="What do I know" />
                    <Button Content="Button"/>
                </StackPanel>
            </Border>
            <GroupBox x:Name="groupBox" Header="GroupBox" Margin="10" />
        </StackPanel>
    </Grid>
</Page>
