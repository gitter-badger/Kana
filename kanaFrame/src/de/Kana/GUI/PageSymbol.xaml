<Page x:Class="KanaFrame.PageSymbol"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KanaFrame"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="PageSymbol"
      x:Name="TestPage">
    <Grid>
        <DockPanel>
            <Border Style="{StaticResource StdBorder}" DockPanel.Dock="Top" HorizontalAlignment="Right">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" >
                    <Button x:Name="btnBack" Content="Back" Click="btnBack_Click"/>
                    <Button x:Name="btnSettings" Content="Settings" Click="btnSettings_Click"/>
                </StackPanel>
            </Border>
            <GroupBox Header="GroupBox" Margin="10" />
        </DockPanel>
    </Grid>
</Page>
