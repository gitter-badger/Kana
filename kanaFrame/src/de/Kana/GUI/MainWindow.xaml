<Window x:Name="window" x:Class="KanaFrame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KanaFrame"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button1" Content="Button" HorizontalAlignment="Stretch" Margin="10" VerticalAlignment="Top"/>
        <Button x:Name="button2" Content="Button" HorizontalAlignment="Stretch" Margin="10" VerticalAlignment="Center"/>
        <Button x:Name="button3" Content="Button" HorizontalAlignment="Stretch" Margin="10" VerticalAlignment="Bottom"/>
    </Grid>
</Window>
