<Page x:Class="KanaFrame.PageMain"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KanaFrame"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="PageMain">

    <Grid>
        <UniformGrid Rows="3" Columns="1">
            <Border >
                <Button x:Name="btnSymbol" IsTabStop="False" Focusable="False" Click="btnSymbol_Click"/>
            </Border >
            <Border >
                <Button IsTabStop="False" Focusable="False"/>
            </Border>
            <Border>
                <Button IsTabStop="False" Focusable="False"/>
            </Border>
        </UniformGrid>
    </Grid>
</Page>
