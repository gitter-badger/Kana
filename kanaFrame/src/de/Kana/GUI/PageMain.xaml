<Page x:Class="KanaFrame.PageMain"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KanaFrame"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="PageMain">
    
    <Grid>
        <UniformGrid Rows="3" Columns="1">
            <Button x:Name="button1" Content="Button" Margin="5" IsTabStop="False" Focusable="False" Click="button1_Click"/>
            <Button x:Name="button2" Content="Button" Margin="5" IsTabStop="False" Focusable="False"/>
            <Button x:Name="button3" Content="Button" Margin="5" IsTabStop="False" Focusable="False"/>
        </UniformGrid>
    </Grid>
</Page>
